<div class="row">
  <div class="col-md-6">
    <ol>
      {{#each model as |number|}}
        Name:  {{number.name}} <br> Serving Size: {{number.measure}} <br>
          {{#each number.nutrients as |nutrient|}}
            {{nutrient.nutrient}}: {{nutrient.value}} {{nutrient.unit}} <br>
          {{/each}}
      {{/each}}
    </ol>
  </div>
  <div class="col-md-6">
    {{from-search item=item saveSearched='saveSearched'}}
  </div>
</div>
